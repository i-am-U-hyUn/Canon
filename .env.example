# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=canon_print_management
POSTGRES_USER=canon_admin
POSTGRES_PASSWORD=your_secure_password

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Spring Boot Configuration
SPRING_PROFILES_ACTIVE=dev
SERVER_PORT=8080
JWT_SECRET=your_jwt_secret_key_here

# .NET Agent Configuration
DOTNET_ENVIRONMENT=Development
MONITORING_AGENT_PORT=5000
SNMP_COMMUNITY=public
SNMP_TIMEOUT=5000

# Email Notification (SMTP)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password
SMTP_FROM=canon-print-alerts@company.com

# Slack Webhook (Optional)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Printer Network Range
PRINTER_NETWORK_RANGE=192.168.1.0/24

# Cost Calculation (원/페이지)
COST_PER_PAGE_BW=30
COST_PER_PAGE_COLOR=150
COST_PER_PAGE_DUPLEX=20

# Policy Engine
AUTO_CONVERT_COLOR_TO_BW=true
FORCE_DUPLEX_PRINTING=true
COLOR_IMAGE_THRESHOLD=0.1
